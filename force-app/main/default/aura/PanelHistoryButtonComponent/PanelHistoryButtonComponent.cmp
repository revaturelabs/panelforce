<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId" access="global">
    <aura:attribute name="failedPanels" type="PH_Assessment__c[]"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <aura:iteration items="{!v.failedPanels}" var="fp">
        <ui:outputText value="{!fp.Name}" /> <br/>
    </aura:iteration>
</aura:component>